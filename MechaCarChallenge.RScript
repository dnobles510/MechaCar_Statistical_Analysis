library(dplyr)
mecha_table <- read.csv("MechaCar_mpg.csv")
all_columns <- lm(mpg ~ vehicle_length + vehicle_weight + spoiler_angle + ground_clearance + AWD, data=mecha_table)
summary(all_columns)
suspension_table <- read.csv("Suspension_Coil.csv")
total_summary <- summarize(suspension_table, mean=mean(PSI), median=median(PSI), var=var(PSI), sd=sd(PSI))
lot_summary <- suspension_table %>% group_by(Manufacturing_Lot) %>% summarize(mean=mean(PSI), median=median(PSI), var=var(PSI), sd=sd(PSI))
t.test(suspension_table$PSI, mu=1500)
